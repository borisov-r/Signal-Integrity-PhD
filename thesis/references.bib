@Article{1132691,
  Title                    = {Characteristics of Metal-Insulator-Semiconductor Coplanar Waveguides for Monolithic Microwave Circuits},
  Author                   = {Sorrentino, R. and Leuzzi, G. and Silbermann, A.},
  Journal                  = {Microwave Theory and Techniques, IEEE Transactions on},
  Year                     = {1984},

  Month                    = {Apr},
  Number                   = {4},
  Pages                    = {410-416},
  Volume                   = {32},

  Abstract                 = {Using a full-wave mode-matching technique, an extensive analysis is presented of the slow-wave factor, attenuation, and characteristic impedance of a metal-insulator semiconductor coplanar waveguide (MISCPW) as functions of the various structural parameters. Design criteria are given for low-attenuation slow-wave propagation. By a proper optimization of the structure, performances comparable with or even better than those of alternative structures proposed in the literature are theoretically predicted.},
  Doi                      = {10.1109/TMTT.1984.1132691},
  ISSN                     = {0018-9480},
  Keywords                 = {Attenuation;Coplanar waveguides;Costs;Electromagnetic waveguides;Gallium arsenide;Magnetic analysis;Magnetic confinement;Metal-insulator structures;Microwave circuits;Structural engineering}
}

