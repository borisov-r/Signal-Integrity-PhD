@Article{554583,
  Title                    = {Rigorous analysis of mode propagation and field scattering in silicon-based coplanar MIS slow wave structures with abrupt transitions to transmission lines on normal substrate},
  Author                   = {Chen, Shuoqi and Vahldieck, R. and Jifu Huang},
